$(function(){
    $(document).ajaxSend(function(){
        $('.loading').height($(this).height());
        $('.loading').show();
        $('body').css('overflow', 'hidden');
        $('.loader').css('margin-top', $(window).height()/2 + $(this).scrollTop() - 60);
    });
    $(document).ajaxComplete(function(){
        $('.loading').hide();
        $('body').css('overflow', 'auto');
    });
});
